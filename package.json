{
  "name": "starwars-xwing",
  "description": "Star Wars X-Wing Sound Effects",
  "version": "0.0.1",
  "author": "Esther Kool",
  "main": "gulpfile.js",
  "dependencies": {
    "browser-sync": "^2.26.7",
    "compass-mixins": "0.12.10",
    "css-mqpacker": "^5.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssfont64": "0.0.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^3.2.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.12.1",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp.spritesmith": "^6.10.1",
    "postcss-flexbugs-fixes": "^2.0.0",
    "run-sequence": "1.2.2"
  },
  "scripts": {
    "gulp": "gulp build"
  },
  "paths": {
    "css": {
      "src": "src/css",
      "dest": "dest/css"
    },
    "sass": {
      "src": "src/sass/",
      "dest": "dest/css/"
    },
    "js": {
      "src": "src/js/",
      "dest": "dest/js/"
    },
    "audio": {
      "src": "src/audio/",
      "dest": "dest/audio/"
    },
    "fonts": {
      "src": "src/fonts/",
      "dest": "dest/fonts/"
    },
    "iconfont": {
      "base": "dest/",
      "conf": "src/conf/",
      "src": "src/img/iconfont/",
      "destScss": "../../src/sass/04-autogenerated/",
      "destFont": "dest/fonts/",
      "destFontRelative": "../fonts/"
    },
    "base64css": {
      "src": "dest/fonts/",
      "destcss": "src/sass/04-autogenerated/"
    },
    "spritePng": {
      "src": "src/img/sprite-png/",
      "src_retina": "src/img/sprite-png/sprite2x/",
      "dest_css": "src/sass/04-autogenerated/",
      "destRelative": "../img/",
      "dest": "dest/img/"
    },
    "spriteSvg": {
      "srcRoot": "src/",
      "conf": "src/conf/",
      "src": "src/img/sprite-svg/",
      "destScss": "sass/04-autogenerated/",
      "destSvg": "../dest/img/"
    },
    "images": {
      "src": "src/img/images/",
      "dest": "dest/img/images/"
    }
  }
}
